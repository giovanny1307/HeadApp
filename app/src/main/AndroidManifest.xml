<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="headup.digitalexperiences.com.headup"
    android:versionCode="2" android:versionName="1.1"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.VIBRATE"/>
    <uses-permission android:name="android.permission.GET_TASKS"/>
    <uses-permission android:name="android.permission.READ_PROFILE"/>
    <uses-permission android:name="android.permission.READ_CONTACTS"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.write_external_storage"/>





    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcherhapp"
        android:label="HeadApp"
        android:theme="@style/AppTheme"
        android:name="headapp.digitalexperiences.com.headapp.HeadAppApplication"
        >

        <meta-data android:name="com.google.android.gms.version"
                   android:value="@integer/google_play_services_version" />


        <receiver
            android:name="headapp.digitalexperiences.com.headapp.Receiver"
            android:enabled="true"
            android:exported="false" >
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />
            </intent-filter>
        </receiver>

        <!--<receiver android:name=".RecibeOnOff">-->
            <!--<intent-filter>-->
                <!--<action android:name="android.intent.action.BOOT_COMPLETED" />-->
            <!--</intent-filter>-->
        <!--</receiver>-->

        <service android:name="headapp.digitalexperiences.com.headapp.BackgroundService" >
        </service>

        <activity
            android:name="headapp.digitalexperiences.com.headapp.HeadAppMain"
            android:label="HeadApp" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>




        <activity android:name="headapp.digitalexperiences.com.headapp.OnBoarding"
            android:label="onboarding"
            android:screenOrientation="portrait"
            />

        <activity android:name="headapp.digitalexperiences.com.headapp.Settings"
            android:label="Settings">
        <intent-filter>
            <action android:name="headapp.digitalexperiences.com.headapp.Settings"/>

            <category android:name="android.intent.category.PREFERENCE"/>
        </intent-filter>

            </activity>

        <activity android:name="com.google.android.gms.ads.AdActivity"
            android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|uiMode|screenSize|smallestScreenSize"
            android:theme="@android:style/Theme.Translucent" />




        <provider
            android:authorities="headapp.digitalexperiences.com.headapp.provider.mensajesprovider"
            android:name="headapp.digitalexperiences.com.headapp.provider.TaskProvider"
            />

    </application>

</manifest>
